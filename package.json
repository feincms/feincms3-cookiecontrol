{
  "name": "feincms3-cookiecontrol",
  "version": "0.0.1",
  "description": "feincms3-cookiecontrol frontend build",
  "repository": "git@github.com:feinheit/feincms3-cookiecontrol.git",
  "author": "York Schickl <ys@feinheit.ch>",
  "license": "BSD-3",
  "private": true,
  "devDependencies": {
    "esbuild": "^0.15.5"
  },
  "scripts": {
    "build": "esbuild feincms3_cookiecontrol/static/feincms3_cookiecontrol/main.* --minify --target=es6 --outdir=feincms3_cookiecontrol/static/feincms3_cookiecontrol/ --entry-names=[dir]/build.[name]",
    "watch": "esbuild feincms3_cookiecontrol/static/feincms3_cookiecontrol/main.* --minify --target=es6 --outdir=feincms3_cookiecontrol/static/feincms3_cookiecontrol/ --entry-names=[dir]/build.[name] --watch"
  },
  "eslintIgnore": [
    "**/build.*"
  ],
  "prettier": {
    "semi": false
  }
}
